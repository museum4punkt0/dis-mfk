{
  "name": "museum",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@smb/display": "^1.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.23",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "copyfiles": "^2.4.1",
    "i18next": "^22.4.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.9.0",
    "react-loader-spinner": "^5.3.4",
    "react-scripts": "5.0.1",
    "sass": "^1.62.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm run prepareGlbContent && react-scripts start",
    "build": "npm run prepareGlbContent && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "react-scripts test --env=jsdom --watchAll=false --coverage",
    "copy": "copyfiles --u 2 src/assets/glb/**/* public/assets",
    "clearGlbFolder": "rm -rf public/assets/glb",
    "prepareGlbContent": "npm run clearGlbFolder && npm run copy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "src/**/*.{ts,tsx}": {
        "statements": 0
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/**/*.d.ts",
      "!src/i18n.ts",
      "!src/api/**",
      "!src/types/**",
      "!src/**/type.ts"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
